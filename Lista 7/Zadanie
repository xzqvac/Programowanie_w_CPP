#include <iostream>
#include <vector>
#include <algorithm> // count_if, sort

using namespace std;

struct Zliczaj_5
{
    bool operator()(int k) const { return k == 5; }
};

struct Sortuj
{
    bool operator()(int i, int j) const {return i % 10 < j % 10;}
};

int main()
{
    std::vector<int> v = {1, 5, 16, 12, 35, 6, 13, 45, 5, 8};

    std::cout << std::count_if(begin(v), end(v), Zliczaj_5()) << "\n";
    std::cout << std::count_if(begin(v), end(v), [](int k) { return k == 5; }) << "\n";
    std::sort(v.begin(), v.end(), Sortuj());
    for(int i = 0; i <= v.size() - 1; i++)
    {
        std::cout << v[i] << " ";
    }
    std::sort(v.begin(), v.end(), [](int i, int j) {return i % 10 < j % 10;});
    for(int i = 0; i <= v.size() - 1; i++)
    {
        std::cout << v[i] << " ";
    }
}


